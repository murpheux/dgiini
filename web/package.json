    "scripts": {
        "ng": "ng",
        "start": "ng serve --base-href --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
        "build": "ng build",
        "compile": "ngc -p tsconfig.json",
        "build-dev": "ng build --base-href",
        "build-prod": "ng build --prod --base-href",
        "build-docker": "ng build --base-href --configuration=docker",
        "build-k8s": "ng build --base-href --configuration=k8s",
        "build-do-k8s": "ng build --base-href --configuration=do.k8s",
        "startx": "npm run config -- --environment=dev && ng serve --environment=dev",
        "test": "ng test --watch=false --browsers ChromeHeadless",
        "lint": "ng lint",
        "tslint": "npx tslint --project tsconfig.json --fix",
        "e2e": "ng e2e",
        "clean": "rimraf dist",
        "config": "ts-node ./src/set-env.ts",
        "postinstall": "node version.js",
        "pretty": "prettier --write \"./**/*.{ts,tsx,json,htm,html}\""
    },
    "private": true,
